{
  "name": "jiracsserver",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "server": "ts-node src/server.ts",
    "server:prod": "npm run build && node dist/server.js",
    "server:dev": "ts-node --transpile-only src/server.ts",
    "test": "ts-node src/tests/index.ts",
    "test:mock": "TEST_MODE=true ts-node src/tests/index.ts",
    "test:api": "TEST_MODE=true ts-node src/tests/api-test.ts"
  },
  "keywords": [
    "jira",
    "langgraph",
    "ai",
    "customer-service"
  ],
  "author": "",
  "license": "ISC",
  "description": "Jira Customer Service Agent Workflow System using LangGraph",
  "dependencies": {
    "@langchain/core": "^0.3.77",
    "@langchain/langgraph": "^0.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.5.2",
    "@types/uuid": "^10.0.0",
    "axios": "^1.12.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "joi": "^18.0.1",
    "morgan": "^1.10.1",
    "openai": "^5.23.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "uuid": "^13.0.0"
  }
}
